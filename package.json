{
    "name": "mdr-cli",
    "version": "0.0.1",
    "description": "",
    "main": "dist/cli.js",
    "author": {
        "name": "Pepijn van den Broek",
        "email": "pepijn@vdbroek.dev",
        "url": "https://vdbroek.dev/"
    },
    "license": "MIT",
    "bin": {
        "mdr": "./dist/cli.js"
    },
    "scripts": {
        "test": "cross-env NODE_ENV=development ts-node ./src/cli.ts",
        "mdr": "cross-env NODE_ENV=development ts-node ./src/cli.ts",
        "prebuild": "rm -rf dist",
        "build": "tsc && chmod +x ./dist/cli.js",
        "format": "prettier --write \"src/**/*.{ts,js}\"",
        "lint": "eslint \"src/**/*.{ts,js}\" --fix",
        "pretty": "npm run format && npm run lint",
        "clean": "rm -rf ./node_modules ./package-lock.json ./yarn.lock",
        "setup": "cp ./.env.example ./.env && npm install && npm run build && npm link"
    },
    "dependencies": {
        "@iarna/toml": "^2.2.5",
        "axios": "^0.20.0",
        "clime": "^0.5.14",
        "dotenv": "^8.2.0",
        "yn": "^4.0.0"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/node": "^14.11.2",
        "@typescript-eslint/eslint-plugin": "^4.3.0",
        "@typescript-eslint/parser": "^4.3.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.10.0",
        "eslint-config-prettier": "^6.12.0",
        "gts": "^2.0.2",
        "nodemon": "^2.0.4",
        "prettier": "^2.1.2",
        "ts-node": "^9.0.0",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.0.3"
    }
}
